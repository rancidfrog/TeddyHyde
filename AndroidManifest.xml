<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.teddyhyde"
          android:versionCode="7"
          android:versionName="0.2.5">
    <uses-sdk android:minSdkVersion="11"/>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />


    <application
            android:icon="@drawable/teddyhyde"
            android:label="@string/app_name">
        <activity android:name=".MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>

            </intent-filter>
        </activity>

        <activity android:name=".RepoListActivity"/>
        <activity android:name=".FileListingActivity"/>
        <activity android:name=".RenderedActivity"/>
        <activity android:name=".ScreenSlideActivity"/>
        <activity android:name=".PixActivity"/>
        <activity android:name=".ScratchpadActivity"/>

        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/app_id"/>

    </application>


</manifest> 
